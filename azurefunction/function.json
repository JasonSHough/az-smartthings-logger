{
    "disabled": false,
    "bindings": [
        {
            "type": "queueTrigger",
            "direction": "in",
            "name": "myQueueItem",
            "queueName": "%ST_QUEUENAME%",
            "connection": "AzureWebJobsStorage"
        },
        {
            "type": "CosmosDB",
            "direction": "out",
            "name": "outputdoc",
            "databaseName": "%CosmosDBName%",
            "collectionName": "%CosmosContainer%",
            "createIfNotExists": true,
            "partitionKey": "/eventId",
            "connectionStringSetting": "CosmosDBConnection"
        }
    ]
}